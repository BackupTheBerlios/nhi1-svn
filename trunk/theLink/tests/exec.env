#!/usr/bin/env bash

## get env
. $(dirname $0)/../../env.sh

LANG=C

[[ $USE_TCL	== 'yes' ]] && set "${@/TCLSH/$TCLSH}"
[[ $USE_CSHARP	== 'yes' ]] && set "${@/MONO/$CLREXEC}"
[[ $USE_JAVA	== 'yes' ]] && set "${@/JAVA/$JAVA}"
[[ $USE_PERL	== 'yes' ]] && set "${@/PERL/$PERL}"
[[ $USE_PHP	== 'yes' ]] && set "${@/PHP/$PHP}"
[[ $USE_PYTHON	== 'yes' ]] && set "${@/PYTHON/$PYTHON}"

eval "$@"

