## get env
. $(dirname $0)/../env.sh

PATH="../src/.libs:../tclmsgque/.libs:../tests/.libs:../tests:${TCLBINDIR}:${PATH:-}"
LD_LIBRARY_PATH="../src/.libs:../tclmsgque/.libs:${LD_LIBRARY_PATH:-}"
TCLLIBPATH="../tclmsgque/.libs"
export PATH LD_LIBRARY_PATH TCLLIBPATH
test "${TCLSH}" != "tclsh" && alias tclsh="${TCLSH}"
shopt -s expand_aliases
eval "$@"

