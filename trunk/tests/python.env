## get env
. $(dirname $0)/../env.sh

PATH="../src/.libs:../pymsgque/.libs:../tests/.libs:../tests:${PATH:-}"
LD_LIBRARY_PATH="../src/.libs:../pymsgque/.libs:../tests/.libs:${LD_LIBRARY_PATH:-}"
PYTHONPATH="../pymsgque/.libs"
export PATH LD_LIBRARY_PATH PYTHONPATH
test "${PYTHON}" != "python" && alias python="${PYTHON}"
shopt -s expand_aliases
eval "$@"

